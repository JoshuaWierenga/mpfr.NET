<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SolutionDir Condition="'$(SolutionDir)' == ''">$(MSBuildThisFileDirectory)</SolutionDir>
    <OutputPath>$(SolutionDir)\bin\$(MSBuildProjectName)\$(Platform)\$(Configuration)\$(TargetFramework)</OutputPath>
    <IntermediateOutputPath>$(SolutionDir)\obj\$(MSBuildProjectName)\$(Platform)\$(Configuration)\$(TargetFramework)</IntermediateOutputPath>
    <OutDir>$(SolutionDir)\bin\$(MSBuildProjectName)\$(Platform)\$(Configuration)\$(TargetFramework)</OutDir>
    <IntDir>$(SolutionDir)\obj\$(MSBuildProjectName)\$(Platform)\$(Configuration)\$(TargetFramework)</IntDir>
  </PropertyGroup>
</Project>